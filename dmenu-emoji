#!/bin/sh

temp=$(mktemp dmenu-emoji.XXXXXX)
sed "0,/^__DATA__$/d" "$0" 2> /dev/null > $temp
data=$(cat "$temp" | dmenu -i -l 20 -p "Emoji ")
echo -n "${data: -1}" | xclip -sel clip && notify-send -t 1500 -u low "${data: -1}" "Copied to Clipboard"

rm -rf $temp
exit

__DATA__
